{
  "build": {
    "content": [
      {
        "src": "docs-ref-autogen",
        "files": [
          "**.yml",
          "**.md"
        ],
        "exclude": [
          "toc.md"
        ],
        "dest": "api"
      },
      {
        "src": "docs-ref-autogen",
        "files": [
          "toc.md"
        ],
        "exclude": [],
        "dest": "api/operationsmanager"
      },
      {
        "src": "docs-ref-conceptual",
        "files": [
          "**.md"
        ],
        "dest": "api"
      },
      {
        "src": ".",
        "files": [
          "index.md"
        ],
        "exclude": [
          "README.md"
        ],
        "dest": "operationsmanager"
      },
      {
        "files": [
          "breadcrumb/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "rest_product": "Operations Manager",
      "titleSuffix": "Operations Manager REST API",
      "breadcrumb_path": "/rest/breadcrumb/toc.json",
      "extendBreadcrumb": true
    },
    "fileMetadata": {
      "ms.service": {
        "docs-ref-autogen/**.yml": "system-center"
      },
      "searchScope": {
        "docs-ref-autogen/**.yml": [
          "Operations Manager",
          "REST"
        ]
      },
      "author": {
        "docs-ref-autogen/**.yml": "jyothirmaisuri"
      },
      "ms.author": {
        "docs-ref-autogen/**.yml": "v-jysur"
      },
      "ms.prod": {
      }
    },
    "template": [],
    "dest": "OpsMgrRestApi",
    "markdownEngineName": "markdig"
  }
}
